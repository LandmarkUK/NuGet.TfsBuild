<?xml version="1.0" encoding="utf-8" ?>
<!--NuGet.TfsBuildService.props v1.0.0 - See https://github.com/LandmarkUK/NuGet.TfsBuild for more information -->
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <PrivateNuGetRepository Condition="$(PrivateNuGetRepository) == ''"></PrivateNuGetRepository>
    <PrivateNuGetRepositoryUsername Condition="$(PrivateNuGetRepositoryUsername) == ''"></PrivateNuGetRepositoryUsername>
    <PrivateNuGetRepositoryPassword Condition="$(PrivateNuGetRepositoryPassword) == ''"></PrivateNuGetRepositoryPassword>

    <NuGetToolsPath Condition="$(NuGetToolsPath) == '' Or $(NuGetToolsPath) == '*Undefined*'">$([System.IO.Path]::Combine($(SolutionDir), ".nuget"))</NuGetToolsPath>
    <NuGetExePath Condition="$(NuGetExePath) == '' Or $(NuGetExePath) == '*Undefined*'">$(NuGetToolsPath)\nuget.exe</NuGetExePath>

  </PropertyGroup>

  <PropertyGroup>
    <BuildDependsOn Condition="$(CreateNuGetPackage) == 'true'">
      InstallPrivateNuGetRepository;
      $(BuildDependsOn);
    </BuildDependsOn>
  </PropertyGroup>
  
</Project>